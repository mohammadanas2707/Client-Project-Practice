.banner{
    background-image: linear-gradient(to right, #2a5298, rgba(30, 60, 114, 0.59)),url(../img/banner.jpg);
    background-size: cover;
}
.aboutme{
	position: relative;
}
h3.about{
	position: absolute;
    font-size: 60px;
    font-weight: 800;
    letter-spacing: 5px;
	transform: rotate(90deg);
    right: -38%;
    top: 112%;
    color: #333;
}
.number{
	color: #2a5298;
}
.servicebox{
	box-shadow: 0px 2px 5px 2px rgba(76, 110, 245, .1);
	background-color: #fff;
}
.worked{
	background-image: linear-gradient(to right, #e36596, rgba(30, 60, 114, 0.59)),url(../img/banner.jpg);
	background-size: cover;
}